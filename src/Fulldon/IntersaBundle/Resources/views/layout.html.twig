    <script>
        $('.datepicker').datepicker({
            language: "fr-FR",
            format: "dd/mm/yyyy"
        });
        $('.datepicker-birth').datepicker({
            startView: 'decade',
            language: "fr-FR",
            format: "dd/mm/yyyy"
        });
        function load_stats() {

            $.ajax({
                url: "/intersa/stats/saisie",
                beforeSend: function (xhr) {
                    ;
                }
            })
                    .done(function (data) {
                        var res = data.split('|')
                        $("#img_stats_saisie").hide();
                        $("#img_stats_saisie2").hide();
                        $('#val_stats_saisie').html(res[0] + ' Lots en attente de finalisation');
                        $('#val_stats_saisie2').html(res[0]);
                        $("#img_stats_verif").hide();
                        $('#val_stats_verif').html(res[1]);
                        $("#img_stats_doublons").hide();
                        $('#val_stats_doublons').html(res[2]);
                        $("#img_stats_anos").hide();
                        $('#val_stats_anos').html(res[3]);
                    });
        }
        {% if  is_granted('ROLE_TRIAL') %}
            var unique_id = $.gritter.add({
                title: 'Félicitations !',
                text: 'Nous sommes disponibles pour vous accompagner durant l\'utilisation de Fulldon <br /> +33654789123 ',
                image: '{{ asset('img/avatar.png') }}',
                sticky: true,
                time: '',
                class_name: 'gritter-warning' + (!$('#gritter-light').get(0).checked ? ' gritter-light' : '')
            });
        {% endif %}
    </script>
