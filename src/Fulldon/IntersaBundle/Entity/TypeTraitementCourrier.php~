namespace Fulldon\IntersaBundle\Entity;

use Doctrine\Common\Collections\ArrayCollection;
use Doctrine\ORM\Mapping as ORM;
use Symfony\Bridge\Doctrine\Validator\Constraints\UniqueEntity;
use Symfony\Component\Validator\Constraints as Assert;


/**
 * @ORM\Table(name="type_traitement_courrier")
 * @ORM\Entity(repositoryClass="Fulldon\IntersaBundle\Entity\TypeTraitementCourrierRepository")
 */
class TypeTraitementCourrier
{
    /**
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id()
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @ORM\Column(name="nom", type="string", length=255)
     */
    protected $name;
    /**
     * @ORM\Column(name="code", type="string", length=255)
     */
    protected $code;
    /**
     * @ORM\Column(name="visible", type="boolean", options = {"default":true})
     */
    protected $visible;

    public function __construct()
    {
        $this->createdAt = new \Datetime();
    }





    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return TypeTraitementCourrier
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }



    /**
     * Set code
     *
     * @param string $code
     * @return TypeTraitementCourrier
     */
    public function setCode($code)
    {
        $this->code = $code;

        return $this;
    }

    /**
     * Get code
     *
     * @return string 
     */
    public function getCode()
    {
        return $this->code;
    }

    /**
     * Set visible
     *
     * @param boolean $visible
     * @return TypeTraitementCourrier
     */
    public function setVisible($visible)
    {
        $this->visible = $visible;

        return $this;
    }

    /**
     * Get visible
     *
     * @return boolean 
     */
    public function getVisible()
    {
        return $this->visible;
    }
}
